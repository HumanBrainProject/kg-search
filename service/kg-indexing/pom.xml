    <name>KG Search Indexing</name>

    <dependencies>
        <dependency>
            <groupId>eu.ebrains.kg.search</groupId>
            <artifactId>common</artifactId>
            <version>3.0.0</version>
        </dependency>

        <dependency>
            <groupId>eu.ebrains.kg.search</groupId>
            <artifactId>project.ebrains</artifactId>

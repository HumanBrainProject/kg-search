    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>eu.ebrains.kg.search</groupId>
        <artifactId>parent</artifactId>
        <version>3.0.0</version>
        <relativePath>../parent</relativePath>
    </parent>
    <artifactId>indexing</artifactId>
